<%= form_with(model: dnc, local: true) do |form| %>
  <% if dnc.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(dnc.errors.count, "error") %> prohibited this dnc from being saved:</h2>

      <ul>
      <% dnc.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :terrid %>
    <%= form.number_field :terrid, id: :dnc_terrid %>
  </div>

  <div class="field">
    <%= form.label :number %>
    <%= form.text_field :number, id: :dnc_number %>
  </div>

  <div class="field">
    <%= form.label :street %>
    <%= form.text_field :street, id: :dnc_street %>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :dnc_name %>
  </div>

  <div class="field">
    <%= form.label :publisher %>
    <%= form.text_field :publisher, id: :dnc_publisher %>
  </div>

  <div class="field">
    <%= form.label :date %>
    <%= form.date_select :date, id: :dnc_date %>
  </div>

  <div class="field">
    <%= form.label :notes %>
    <%= form.text_field :notes, id: :dnc_notes %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
